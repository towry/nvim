{
  "Prettier ignore comment": {
    "prefix": ["ignore_prettier", "prettier_ignore"],
    "body": [
      "// prettier-ignore"
    ]
  },
  "Eslint disable next line": {
    "prefix": ["eslint_disable"],
    "body": [
      "// eslint-disable-next-line"
    ]
  },
  "generate snippet package": {
    "prefix": ["snippet_package"],
    "body": [
      "{",
      " \"name\": \"user-snippets\",",
      " \"description\": \"User snippets for VS Code\",",
      " \"contributes\": {",
      "   \"snippets\": [",
      "     {",
      "       \"language\": \"all\",",
      "       \"path\": \"./all.json\"",
      "     }",
      "    ]",
      "  }",
      "}"
    ]
  }
}
