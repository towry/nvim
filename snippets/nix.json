{
  "nixpkgs-stable-inputs + flake-nixpkgs": {
    "body": "nixpkgs-stable.url = \"github:nixos/nixpkgs?ref=24.05\";",
    "prefix": [
      "nixpkgs-stable-inputs",
      "flake-nixpkgs"
    ]
  },
  "nixpkgs-unstable-url": {
    "body": "github:nixos/nixpkgs?ref=nixpkgs-unstable",
    "prefix": [
      "nixpkgs-unstable-url"
    ]
  },
  "nixpkgs-stable-url": {
    "body": "github:nixos/nixpkgs?ref=24.05",
    "prefix": [
      "nixpkgs-stable-url"
    ]
  },
  "inputs-follows-nixpkgs": {
    "body": "inputs.${1:nixpkgs}.follows = \"${2:nixpkgs}\";",
    "prefix": [
      "inputs-follows-nixpkgs"
    ]
  }
}
