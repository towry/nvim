{
  "snippets.ultisnips.pythonPrompt": false,
  "semanticTokens.enable": true,
  "volar.useWorkspaceTsdk": true,
  "coc.preferences.formatOnSave": false,
  "coc.preferences.useQuickfixForLocations": true,
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.virtualText": true,
  "suggest.floatConfig": {
    "border": true
  },
  "diagnostic.floatConfig": {
    "border": true
  },
  "pullDiagnostic.onSave": false,
  "hover.floatConfig": {
    "border": true
  },
  "workspace.ignoredFolders": ["$HOME", "$HOME/.cargo/**", "$HOME/.rustup/**"],
  "rust-analyzer.lens.enable": false,
  "rust-analyzer.inlayHints.maxLength": 2,
  "rust-analyzer.inlayHints.renderColons": true
}
