{
  "snippets.ultisnips.pythonPrompt": false,
  "semanticTokens.enable": true,
  "volar.useWorkspaceTsdk": true,
  "coc.preferences.formatOnSave": false,
  "coc.preferences.useQuickfixForLocations": true,
  "suggest.noselect": true,
  "suggest.insertMode": "replace",
  "suggest.enablePreselect": false,
  "suggest.virtualText": true,
  "suggest.floatConfig": {
    "border": true
  },
  "diagnostic.floatConfig": {
    "border": true
  },
  "diagnostic.errorSign": "~",
  "diagnostic.warningSign": "~",
  "diagnostic.hintSign": "~",
  "diagnostic.infoSign": "~",
  "pullDiagnostic.onSave": false,
  "hover.floatConfig": {
    "border": true
  },
  "workspace.ignoredFolders": ["$HOME", "$HOME/.cargo/**", "$HOME/.rustup/**"],
  "rust-analyzer.lens.enable": true,
  "rust-analyzer.inlayHints.expressionAdjustmentHints.enable": "reborrow",
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.lens.run.enable": true,
  "rust-analyzer.lens.debug.enable": true,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
  "rust-analyzer.inlayHints.maxLength": 2,
  "rust-analyzer.inlayHints.renderColons": true
}
